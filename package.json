{
  "name": "osrs-web-scraper",
  "version": "0.27.1",
  "description": "Tools to scrape the osrs website.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && ts-patch install -s && tsc",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier \"src/**/*.{js,ts}\"",
    "prettier:fix": "npm run prettier --write",
    "start": "tsx ./src/index.ts",
    "start:debug": "tsx watch ./src/index.ts",
    "start:node": "node -r typescript-transform-paths/register ./dist/src/index.js",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --watch",
    "test:e2e": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --config e2e/jest.config.js",
    "test:e2e:update": "npm run test:e2e -- -u",
    "release": "changeset publish",
    "prepare": "husky install && ts-patch install -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenkinzalow/osrs-web-scraper.git"
  },
  "author": "Allen Kinzalow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allenkinzalow/osrs-web-scraper/issues"
  },
  "homepage": "https://github.com/allenkinzalow/osrs-web-scraper#readme",
  "dependencies": {
    "@osrs-wiki/mediawiki-builder": "^1.9.0",
    "commander": "^13.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "file-type": "^21.0.0",
    "image-size": "^1.2.1",
    "node-html-parser": "^6.1.10",
    "puppeteer": "^23.11.1",
    "rss-parser": "^3.13.0",
    "tsconfig-paths": "^4.2.0",
    "typescript-transform-paths": "^3.4.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@changesets/cli": "^2.29.6",
    "@types/jest": "^28.1.7",
    "@types/node": "^17.0.45",
    "@types/xml2js": "^0.4.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "husky": "^7.0.4",
    "jest": "^28.1.2",
    "lint-staged": "^12.4.0",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.8",
    "ts-patch": "^2.1.0",
    "tsx": "^4.20.5",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "npm run prettier:fix"
    ]
  }
}
